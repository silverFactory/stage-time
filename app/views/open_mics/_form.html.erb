<%= form_for @open_mic do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :genre %>
<%= f.text_field :genre %><br>
<%= f.label :location %>
<%= f.text_field :location %><br>
<%= f.label :day %>
<%= f.text_field :day %><br>
<%= f.label :time %>
<%= f.text_field :time %><br>
<%= f.label :stage_time %>
<%= f.text_field :stage_time %><br>
<%= f.label :sign_up_protocol %>
<%= f.text_field :sign_up_protocol %><br>
<%= f.label :cover %>
<%= f.number_field :cover %><br>
<%= f.label :drink_or_food_minimum %>
<%= f.text_field :drink_or_food_minimum %><br>
<%= f.submit %>
<% end %>
