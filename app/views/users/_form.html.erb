<%= form_for @user do |f| %>
<%=f.label :username %>
<%=f.text_field :username %><br>
<%=f.label :password %>
<%=f.password_field :password %><br>
<%=f.label :password_confirmation %>
<%=f.password_field :password_confirmation %><br>
<%=f.label :stage_name %>
<%=f.text_field :stage_name %><br>
<%=f.label :pic %>
<%=f.text_field :pic %><br>
<%=f.label :bio %>
<%=f.text_area :bio %><br>
<%=f.label :email %>
<%=f.text_field :email %><br>
<%=f.label :email_public %>
<%=f.check_box :email_public %><br>
<%=f.label :facebook_link %>
<%=f.text_field :facebook_link %><br>
<%=f.label :instagram_link %>
<%=f.text_field :instagram_link %><br>
<%=f.label :youtube_link %>
<%=f.text_field :youtube_link %><br>
<%=f.label :bandcamp_link %>
<%=f.text_field :bandcamp_link %><br>
<%=f.label :spotify_link %>
<%=f.text_field :spotify_link %><br>
<%=f.label :host, "Do you host an open mic?" %>
<%=f.check_box :host %><br>
<%= f.submit %>
<% end %>
